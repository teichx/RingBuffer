<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/teichx/TingTuffer</PackageProjectUrl>
    <RepositoryUrl>https://github.com/teichx/TingTuffer</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
	  <Authors>Tem√≠stocles Schwartz</Authors>
	  <Description>a ring buffer for .net</Description>
    <PackageIconUrl>https://github.com/teichx/TingTuffer/blob/master/Assets/icon-ting-tuffer.png?raw=true</PackageIconUrl>
    <NoWarn>NU5048</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
